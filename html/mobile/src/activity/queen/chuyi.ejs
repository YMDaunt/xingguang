    <section><img src="${require('../../../../../static_guojiang_tv/src/mobile/v2/img/activity/queen/chuyi/2.jpg')}"></section>
    <section>
        <img src="${require('../../../../../static_guojiang_tv/src/mobile/v2/img/activity/queen/chuyi/3.jpg')}">
        <span class="go_room_btn" @click="goRoom()"></span>
    </section>
    <section>
        <img src="${require('../../../../../static_guojiang_tv/src/mobile/v2/img/activity/queen/chuyi/4.jpg')}">
